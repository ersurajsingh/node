{
  "name": "node-microservice-template",
  "version": "1.0.0",
  "description": "This is template for microservices in node.js.",
  "main": "server.js",
  "scripts": {
    "start": "babel-node ./server.js",
    "test": "cross-env NODE_ENV=test mocha -r @babel/register -r regenerator-runtime/runtime --reporter json --reporter-options output=report.json --exit",
    "nodemon": "nodemon --exec babel-node ./server.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "prettier": "prettier --check '**/*.js'",
    "prettier:fix": "prettier --write '**/*.js'",
    "prepare": "husky install"
  },
  "author": "Leewayhertz",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "aws-sdk": "^2.1326.0",
    "axios": "^1.3.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "luxon": "^3.2.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "node-cron": "^3.0.2",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/register": "^7.21.0",
    "@types/luxon": "^3.2.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "lint-staged": "10.2.11",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.7",
    "regenerator-runtime": "^0.13.11"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier:fix",
      "npm run lint:fix",
      "npm run lint",
      "npm run test"
    ]
  }
}
